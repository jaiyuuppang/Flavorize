{
    "name": "jaiyuppang/flavorize",
    "description": "A personalized food recommendation app",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",               
        "ext-json": "*",
        "ext-mysqli": "*" ,            
        "vlucas/phpdotenv": "^5.4",   
        "symfony/var-dumper": "^5.0", 
        "twig/twig": "^3.0",
        "openai-php/client": "^0.10.3",
        "guzzlehttp/guzzle": "^7.9"           
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        
    },
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
